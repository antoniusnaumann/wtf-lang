package wtf:example@0.1.0

export func main() -> s64 {
    let y = 0
    return y.s64()
}

test "UFCS returns the same value as function call" {
    let value = 42.3
    assert value.s32() == s32(value)
}

test "UFCS is chainable" {
    let array = [12, 17, 38, 55]
    assert array.len() == len(array)
}
